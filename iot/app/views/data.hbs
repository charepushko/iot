
      <div class="row" style="padding-top: 40px;"></div>
 

      <div class="row" style="padding-top: 20px;">
         <button type="button" class="btn btn-info" onclick="history.back()"><i class="fas fa-angle-double-left"></i> Back </button>
         <div class="col-sm-1"></div>

         <div class="btn-group"  role="group" aria-label="Button group" width=100%>

             <button type="button" class="btn btn-primary">{{provider}} /</button>

             <div class="btn-group" role="group">
               <button id="btnGroupDrop1" type="button" class="btn btn-light dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                 Select device...
               </button>
               <div class="dropdown-menu" aria-labelledby="btnGroupDrop1">
                 <a class="dropdown-item" href="#">Device 1</a>
                 <a class="dropdown-item" href="#">Device 2</a>
               </div>
             </div>

         </div>
         <div class="col-sm-4"></div>
         <button type="button" onclick="window.location.reload()" class="btn btn-info">Refresh <i class="fas fa-sync-alt"></i> </button>



      </div>


<div class="row"  style="padding-top: 20px;">

        <div class="table-responsive">
            <table class="table table-striped table-sm">
              <thead>
                <tr>
                  <th>Device Id</th>
                  <th>Timestamp</th>
                  <th>Hex Data</th>
                  <th>may be</th>
                  <th>somethng else</th>
                </tr>
              </thead>
              <tbody>
               {{#each data}}
                <tr>
                  <td>{{device_id}}</td>
                  <td>{{timestamp}}</td>
                  <td>{{payload_hex}}</td>
                  <td>very long data to make timestamp shorten himself</td>
                  <td>{{time}}</td>
                </tr>
               {{/each}}
              </tbody>
            </table>
           </div>
</div>


